{
  "name": "colorpicker",
  "displayName": "colorpicker",
  "description": "color picker for developers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "colorpicker.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "colorpicker.insertColor",
        "title": "Insert Color"
      },
      {
        "command": "colorpicker.insertColorWithPicker",
        "title": "Insert Color with Picker"
      },
      {
        "command": "colorpicker.showAdvancedColorPicker",
        "title": "Insert Color with Advanced Color Picker"
      }
    ],
    "keybindings": [
      {
        "command": "colorpicker.insertColorWithPicker",
        "key": "ctrl+alt+c",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "colorpicker.insertColorWithPicker",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}